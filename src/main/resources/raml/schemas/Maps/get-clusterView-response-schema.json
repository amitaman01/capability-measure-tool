{
	"$schema": "http://json-schema.org/draft-03/schema#",
	"title": "Industry Map GET Response Schema",
	"description": "schema for a generic list of capabilities ",
	"definitions": {
		"sizeType": {
			"type": "string"
		}
	},
	"type": "object",
	"required": true,
	"properties": {
		"size": {
			"$ref": "#/definitions/sizeType",
			"required": true
		},
		"ClusterView": {
			"type": "array",
			"required": true,
			"items": {
				"type": "object",
				"required": true,
				"properties": {
					"capabilityId": {
						"type": "string",
						"required": true
					},
					"mapId": {
						"type": "string",
						"required": true
					},
					"versionId": {
						"type": "string",
						"required": true
					},
					"sectorId": {
						"type": "string",
						"required": true
					},
					"clusterId": {
						"type": "string",
						"required": true
					},
					"capNotes": {
						"type": "string",
						"required": true
					},
					"capabilityDesc": {
						"type": "string",
						"required": true
					},
					"capabilityNm": {
						"type": "string",
						"required": true
					},
					"capabilityPos": {
						"type": "string",
						"required": true
					},
					"highValue": {
						"type": "string",
						"required": true
					},
					"createDt": {
						"type": "string",
						"required": true
					},
					"lastUpdateDt": {
						"type": "string",
						"required": true
					},
					"truncCapabilityNm": {
						"type": "string",
						"required": true
					},
					"usrNmLastUpd": {
						"type": "string",
						"required": true
					}
				},
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}