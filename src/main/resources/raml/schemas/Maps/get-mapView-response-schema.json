{
	"$schema": "http://json-schema.org/draft-03/schema#",
	"title": "Industry Map GET Response Schema",
	"description": "schema for a generic list of mapview ",
	"definitions": {
		"sizeType": {
			"type": "string"
		}
	},
	"type": "object",
	"required": true,
	"properties": {
		"size": {
			"$ref": "#/definitions/sizeType",
			"required": true
		},
      "MapView":{  
         "type":"array",
         "required":true,
         "items":{  
            "type":"object",
            "required":true,
            "properties":{  
               "clusterId":{  
                  "type":"string",
                  "required":true
               },
               "mapId":{  
                  "type":"string",
                  "required":true
               },
               "versionId":{  
                  "type":"string",
                  "required":true
               },
               "sectorId":{  
                  "type":"string",
                  "required":true
               },
               "clusterNm":{  
                  "type":"string",
                  "required":true
               },
               "clusterPos":{  
                  "type":"string",
                  "required":true
               },
               "createDt":{  
                  "type":"string",
                  "required":true
               },
               "lastUpdateDt":{  
                  "type":"string",
                  "required":true
               },
               "usrNmLastUpd":{  
                  "type":"string",
                  "required":true
               },
               "valueChainId":{  
                  "type":"string",
                  "required":true
               }
            },
            "additionalProperties":false
         }
      }
   },
   "additionalProperties":false
}