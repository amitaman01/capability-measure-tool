{
	"$schema": "http://json-schema.org/draft-03/schema#",
	"title": "Industry Map GET Response Schema",
	"description": "schema for a generic list of IndustryMaps ",
	"definitions": {
		"sizeType": {
			"type": "string"
		}
	},
	"type": "object",
	"required": true,
	"properties": {
		"size": {
			"$ref": "#/definitions/sizeType",
			"required": true
		},
		
		"Map": {
			"type": "array",
			"required": true,
			"items": {
				"type": "object",
				"required": true,
				"properties": {
					"mapId": {
						"type": "string",
						"required": true
					},
					"versionId": {
						"type": "string",
						"required": true
					},
					"sectorId": {
						"type": "string",
						"required": true
					},
					"archiveInd": {
						"type": "string",
						"required": true
					},
					"createDt": {
						"type": "string",
						"required": true
					},
					"lastUpdateDt": {
						"type": "string",
						"required": true
					},
					"mapDesc": {
						"type": "string",
						"required": true
					},
					"mapLastUpdateDt": {
						"type": "string",
						"required": true
					},
					"mapNm": {
						"type": "string",
						"required": true
					},
					"mapStatus": {
						"type": "string",
						"required": true
					},
					"mapTyp": {
						"type": "string",
						"required": true
					},
					"sectorName": {
						"type": "string",
						"required": true
					},
					"usrNmLastUpd": {
						"type": "string",
						"required": true
					},
					"isPrivateMap": {
						"type": "boolean",
						"required": true
					}
				},
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}